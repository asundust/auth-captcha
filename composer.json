{
    "name": "asundust/auth-captcha",
    "description": "Sliding captcha for Laravel-Admin auth, Multiple platform support / Laravel-Admin登录 滑动验证插件 多平台支持",
    "type": "library",
    "keywords": [
        "laravel-admin",
        "extension",
        "sliding captcha",
        "auth",
        "login",
        "captcha",
        "auth captcha",
        "multiple platform"
    ],
    "homepage": "https://github.com/asundust/auth-captcha",
    "license": "MIT",
    "authors": [
        {
            "name": "asundust",
            "email": "asundust@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "ext-json": "*",
        "encore/laravel-admin": "~1.6",
        "guzzlehttp/guzzle": "^6.3||^7.2",
        "jenssegers/agent": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Asundust\\AuthCaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asundust\\AuthCaptcha\\AuthCaptchaServiceProvider"
            ]
        }
    }
}
